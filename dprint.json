{
  "malva": {
    "hexCase": "lower",
    "quotes": "alwaysSingle",
    "blockSelectorLinebreak": "always",
    "omitNumberLeadingZero": false,
    "formatComments": true,
    "alignComments": true
  },
  "markup": {
    "quotes": "single"
  },
  "markdown": {
    "lineWidth": 70,
    "textWrap": "always"
  },
  "typescript": {
    "quoteStyle": "preferSingle",
    "semiColons": "asi"
  },
  "excludes": [],
  "plugins": [
    "https://plugins.dprint.dev/markdown-0.18.0.wasm",
    "https://plugins.dprint.dev/typescript-0.94.0.wasm",
    "https://plugins.dprint.dev/g-plane/markup_fmt-v0.19.1.wasm",
    "https://plugins.dprint.dev/g-plane/malva-v0.11.2.wasm"
  ]
}
